    rdfs:label "전류 센서"@ko .

upw:VoltageSensor a owl:Class ;
    rdfs:subClassOf upw:UPWSensor ;
    rdfs:label "Voltage Sensor"@en ;
    rdfs:label "전압 센서"@ko .

upw:LevelSensor a owl:Class ;
    rdfs:subClassOf upw:UPWSensor ;
    rdfs:label "Level Sensor"@en ;
