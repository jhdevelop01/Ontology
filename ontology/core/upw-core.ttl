# =============================================================================
# UPW (Ultrapure Water) Process Ontology - Core Schema
# =============================================================================

<http://example.org/upw> a owl:Ontology ;
    rdfs:label "UPW Process Predictive Maintenance Ontology"@en ;
    rdfs:label "초순수 공정 예지보전 온톨로지"@ko ;
    rdfs:comment "Ontology for predictive maintenance and energy prediction in ultrapure water (UPW) manufacturing processes"@en ;
    owl:versionInfo "1.0.0" ;
    owl:imports <http://www.w3.org/ns/sosa/> .

# =============================================================================
# CLASSES - Equipment Hierarchy
# =============================================================================

upw:Equipment a owl:Class ;
    rdfs:subClassOf saref:Device ;
    rdfs:label "Equipment"@en ;
    rdfs:label "설비"@ko ;
    rdfs:comment "A physical device or machine used in the UPW production process"@en .

# Pretreatment Equipment
upw:PretreatmentEquipment a owl:Class ;
    rdfs:subClassOf upw:Equipment ;
    rdfs:label "Pretreatment Equipment"@en ;
    rdfs:label "전처리 설비"@ko .

upw:SandFilter a owl:Class ;
    rdfs:subClassOf upw:PretreatmentEquipment ;
    rdfs:label "Sand Filter"@en ;
    rdfs:label "사여과기"@ko .

upw:ActivatedCarbonFilter a owl:Class ;
    rdfs:subClassOf upw:PretreatmentEquipment ;
    rdfs:label "Activated Carbon Filter"@en ;
    rdfs:label "활성탄 필터"@ko .

upw:SedimentFilter a owl:Class ;
    rdfs:subClassOf upw:PretreatmentEquipment ;
    rdfs:label "Sediment Filter"@en ;
    rdfs:label "침전물 필터"@ko .

# Treatment Equipment
upw:TreatmentEquipment a owl:Class ;
    rdfs:subClassOf upw:Equipment ;
    rdfs:label "Treatment Equipment"@en ;
    rdfs:label "처리 설비"@ko .

upw:ReverseOsmosis a owl:Class ;
    rdfs:subClassOf upw:TreatmentEquipment ;
    rdfs:label "Reverse Osmosis Unit"@en ;
    rdfs:label "역삼투압 장치 (RO)"@ko ;
    rdfs:comment "High-pressure membrane filtration system for removing dissolved solids"@en .

upw:Electrodeionization a owl:Class ;
    rdfs:subClassOf upw:TreatmentEquipment ;
    rdfs:label "Electrodeionization Unit"@en ;
    rdfs:label "전기탈이온 장치 (EDI)"@ko ;
    rdfs:comment "Continuous deionization using ion exchange resins and electric current"@en .

upw:Distiller a owl:Class ;
    rdfs:subClassOf upw:TreatmentEquipment ;
    rdfs:label "Distiller"@en ;
    rdfs:label "증류 장치"@ko .

# Post-treatment Equipment
upw:PostTreatmentEquipment a owl:Class ;
    rdfs:subClassOf upw:Equipment ;
    rdfs:label "Post-treatment Equipment"@en ;
    rdfs:label "후처리 설비"@ko .

upw:UVSterilizer a owl:Class ;
    rdfs:subClassOf upw:PostTreatmentEquipment ;
    rdfs:label "UV Sterilizer"@en ;
    rdfs:label "UV 살균기"@ko ;
    rdfs:comment "Ultraviolet light disinfection system (254nm wavelength)"@en .

upw:OzoneGenerator a owl:Class ;
    rdfs:subClassOf upw:PostTreatmentEquipment ;
    rdfs:label "Ozone Generator"@en ;
    rdfs:label "오존 발생기"@ko .

upw:DosingPump a owl:Class ;
    rdfs:subClassOf upw:PostTreatmentEquipment ;
    rdfs:label "Dosing Pump"@en ;
    rdfs:label "약품 주입 펌프"@ko .

# Support Equipment
upw:SupportEquipment a owl:Class ;
    rdfs:subClassOf upw:Equipment ;
    rdfs:label "Support Equipment"@en ;
    rdfs:label "보조 설비"@ko .

upw:CirculationPump a owl:Class ;
    rdfs:subClassOf upw:SupportEquipment ;
    rdfs:label "Circulation Pump"@en ;
    rdfs:label "순환 펌프"@ko .

upw:StorageTank a owl:Class ;
    rdfs:subClassOf upw:SupportEquipment ;
    rdfs:label "Storage Tank"@en ;
    rdfs:label "저장 탱크"@ko .

