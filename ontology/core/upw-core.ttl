# =============================================================================
# OBJECT PROPERTIES
# =============================================================================

upw:hasSensor a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:range upw:UPWSensor ;
    rdfs:label "has sensor"@en ;
    rdfs:label "센서를 가짐"@ko .

upw:isAttachedTo a owl:ObjectProperty ;
    owl:inverseOf upw:hasSensor ;
    rdfs:domain upw:UPWSensor ;
    rdfs:range upw:Equipment ;
    rdfs:label "is attached to"@en .

upw:hasObservation a owl:ObjectProperty ;
    rdfs:domain upw:UPWSensor ;
    rdfs:range upw:SensorObservation ;
    rdfs:label "has observation"@en .

upw:observedProperty a owl:ObjectProperty ;
    rdfs:subPropertyOf sosa:observedProperty ;
    rdfs:domain upw:SensorObservation ;
    rdfs:range upw:ObservableProperty ;
    rdfs:label "observed property"@en .

upw:hasFeatureOfInterest a owl:ObjectProperty ;
    rdfs:subPropertyOf sosa:hasFeatureOfInterest ;
    rdfs:domain upw:SensorObservation ;
    rdfs:range upw:Equipment ;
    rdfs:label "has feature of interest"@en .

upw:madeBySensor a owl:ObjectProperty ;
    rdfs:subPropertyOf sosa:madeBySensor ;
    rdfs:domain upw:SensorObservation ;
    rdfs:range upw:UPWSensor ;
    rdfs:label "made by sensor"@en .

upw:hasUnit a owl:ObjectProperty ;
    rdfs:domain upw:SensorObservation ;
    rdfs:range upw:Unit ;
    rdfs:label "has unit"@en .

upw:hasFailureMode a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:range upw:FailureMode ;
    rdfs:label "has failure mode"@en ;
    rdfs:label "고장 모드를 가짐"@ko .

upw:requiresMaintenance a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:range upw:MaintenanceActivity ;
    rdfs:label "requires maintenance"@en .

upw:triggeredBy a owl:ObjectProperty ;
    rdfs:domain upw:MaintenanceActivity ;
    rdfs:range upw:FailureMode ;
    rdfs:label "triggered by"@en .

upw:hasHealthStatus a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:range upw:HealthStatus ;
    rdfs:label "has health status"@en .

upw:hasAnomalyType a owl:ObjectProperty ;
    rdfs:domain upw:AnomalyDetection ;
    rdfs:range upw:AnomalyType ;
    rdfs:label "has anomaly type"@en .

upw:relatedEquipment a owl:ObjectProperty ;
    rdfs:domain upw:AnomalyDetection ;
    rdfs:range upw:Equipment ;
    rdfs:label "related equipment"@en .

upw:relatedObservation a owl:ObjectProperty ;
    rdfs:domain upw:AnomalyDetection ;
    rdfs:range upw:SensorObservation ;
    rdfs:label "related observation"@en .

upw:hasComponent a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:range upw:Equipment ;
    rdfs:label "has component"@en .

upw:isComponentOf a owl:ObjectProperty ;
    owl:inverseOf upw:hasComponent ;
    rdfs:label "is component of"@en .

upw:locatedIn a owl:ObjectProperty ;
    rdfs:domain upw:Equipment ;
    rdfs:label "located in"@en .

